{
  "name": "stevebauman/administration",
  "description": "An administration backend scaffolding package for Laravel.",
  "keywords": ["authorization", "laravel", "permissions", "roles", "acl"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "~5.2",
    "orchestra/html": "~3.2",
    "stevebauman/authorization": "1.2.*",
    "stevebauman/flash": "1.0.*",
    "stevebauman/active": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "orchestra/testbench": "~3.2"
  },
  "autoload": {
    "psr-4": {
      "Stevebauman\\Administration\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Stevebauman\\Administration\\Tests\\": "tests/"
    },
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
